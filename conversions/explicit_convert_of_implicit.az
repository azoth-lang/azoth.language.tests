// exit code: 2
public fn main() -> byte
{
    // demonstrates using explicit conversions to do implicit conversions
    var b: byte = 2;
    var s: size = 3;
    var o: offset = 5;
    var i1 = b as int;
    var i2 = s as int;
    var i3 = o as int;
    var u1 = b as uint;
    var u2 = s as uint;
    return (i1 * i2 * i3 == u1 * u2 * o) as byte;
}
