public fn test_math(a: offset, b: offset)
{
    let x = a + b;
    let x = a - b;
    let x = a * b;
    let x = a / b;
    let x = a.remainder(b);

    var y: offset = 0;
    y += a;
    y -= a;
    y *= a;
    y /= a;

    let c = a == b;
    let c = a =/= b;
    let c = a > b;
    let c = a >= b;
    let c = a < b;
    let c = a <= b;
}

public fn main()
{
    test_math(2, 1);
}
