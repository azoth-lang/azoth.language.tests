// exit code: 42

public mut class Inner
{
    public var value: byte;

    public new()
    {
        self.value = 1;
    }
}

public mut class Outer
{
    public var inner: mut Inner;

    public new()
    {
        self.inner = new Inner();
    }
}

public fn main() -> byte
{
    let t: mut Outer = new Outer();
    t.inner.value = 42;
    return t.inner.value;
}
