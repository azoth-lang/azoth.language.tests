// exit code: 1

public fn main() -> byte
{
    let box1: iso = new IntBox();
    let box2 = increment(move box1);
    if id box1 == id box2 => return 1
    else => return 0;
}

public class IntBox
{
    public var value: int;
}

public fn increment(box: iso IntBox) -> iso IntBox
{
    box.value += 1;
    return box;
}
